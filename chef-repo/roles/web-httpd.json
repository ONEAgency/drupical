{
  "name": "web",
  "description": "Web role",
  "json_class": "Chef::Role",
  "default_attributes": {
    "memcached": {
      "memory": "256"
    },
    "apache": {
      "default_modules": [
        "alias",
        "cgi",
        "deflate",
        "dir",
        "log_config",
        "mime",
        "proxy",
        "proxy_http",
        "rewrite",
        "setenvif",
        "ssl"
      ],
      "mpm": "worker",
      "listen_ports": [
        80,
        443
      ]
    }
  },
  "chef_type": "role",
  "run_list": [
    "recipe[web-httpd]"
  ],
  "env_run_lists": {
  }
}
