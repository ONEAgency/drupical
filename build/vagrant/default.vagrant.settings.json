{
  "config": {

    "box_type": "hashicorp/precise64",
    "box_name": "drupicaldev",
    "box_hostname": "drupicaldev",
    "box_gui": false,

    "box_dhcp": true,
    "box_static_ip": "10.0.0.1", // ignored if box_dhcp == true

    "box_ram_cpu": "auto",
    "box_ram": "1G", // ignored if box_ram_cpu == auto
    "box_cpu": "2",  // ignored if box_ram_cpu == auto

    "vagrant_debugging": true,

    "drupical": {
      "php" : {
        "php_version": "5.4",
        "enable_php_apc": true,
        "enable_php_curl": true,
        "enable_php_memcache": true,
        "enable_php_drush": true,
        "enable_php_phing": true,
        "enable_php_composer": true,
        "enable_php_xdebug": true
      },
      "solr": {
        "solr_install": false,
        "solr_version": "3.6.2",
        "solr_checksum": "537426dcbdd0dc82dd5bf16b48b6bcaf87cb4049c1245eea8dcb79eeaf3e7ac6"
      },
      "web_tools" : {
        "adminer" : {
          "adminer_install" : true,
          "adminer_alias" : "adminer.drupicaldev.local"
        },
        "apc" : {
          "apc_install" : true,
          "apc_alias" : "apc.drupicaldev.local"
        },
        "phpmemcachedadmin" : {
          "phpmemcachedadmin_install" : true,
          "phpmemcachedadmin_alias" : "phpmemcachedadmin.drupicaldev.local"
        }
      },
      "varnish_install": false
    },

    "vagrant_ports" : {
      "http" : {
        "host" : "80",
        "guest" : "80"
      },
      "https" : {
        "host" : "443",
        "guest" : "443"
      },
      "xdebug" : {
        "host" : "9000",
        "guest" : "9000"
      },
      "varnish" : {
        "host" : "8080",
        "guest" : "8080"
      },
      "mysql" : {
        "host" : "3306",
        "guest" : "3306"
      },
      "solr" : {
        "host" : "8390",
        "guest" : "8390"
      }
    },

    "vagrant_synced_folders" : {
      "build" : {
        "source" : "../drupical",
        "target" : "/home/vagrant/drupical",
        "type" : "nfs",
        "mount_options" : ["rw", "vers=3", "tcp", "fsc", "actimeo=1"]
      }
    },

    "vhosts" : {
      "vhost1" : {
        "server_name" : "drupicaldev.local",
        "database_name" : "drupical",
        "docroot" : "/home/vagrant/drupical/docroot/",
        "aliases" : {
          "alias1" : "www.drupicaldev.local"
        }
      }
    }

  }
}
