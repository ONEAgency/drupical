{
  "config": {

    "vagrant_debugging": true,

    "vagrant_synced_folders" : {
      "build" : {
        "source" : "../drupical",
        "target" : "/home/vagrant/drupical",
        "type" : "nfs",
        "mount_options" : ["rw", "vers=3", "udp", "fsc", "actimeo=1"]
      },
      "adminer" : {
        "source" : "./build/web-tools/adminer/",
        "target" : "/usr/share/adminer/",
        "type" : "nfs",
        "mount_options" : ["rw", "vers=3", "udp", "fsc", "actimeo=1"]
      },
      "siteinfo" : {
        "source" : "./build/web-tools/siteinfo/",
        "target" : "/usr/share/siteinfo/",
        "type" : "nfs",
        "mount_options" : ["rw", "vers=3", "udp", "fsc", "actimeo=1"]
      }
    },

    "vhosts" : {
      "drupical" : {
        "server_name" : "drupicaldev.local",
        "database_name" : "drupical",
        "docroot" : "/home/vagrant/drupical/docroot/",
        "aliases" : [ "www.drupicaldev.local" ]
      }
    }

  }
}